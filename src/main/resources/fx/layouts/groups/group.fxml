<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<HBox prefHeight="80.0" prefWidth="400.0" spacing="10.0" styleClass="group" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <ImageView fx:id="group_icon" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true" styleClass="group_icon" />
      <VBox prefHeight="61.0">
         <children>
            <Label fx:id="group_name" styleClass="group_name" text="Label" />
            <Label fx:id="group_members" prefHeight="43.0" styleClass="group_members" text="Label" />
         </children>
      </VBox>
   </children>
</HBox>
